[user]
	email = mogurijin@gmail.com
	name = Mitchell Stokes
[push]
	default = simple
[credential]
	helper = cache
[svn]
	pushmergeinfo = true
[core]
	autocrlf = input
	editor = vim
[alias]
	co = checkout
	sl = stash list
    ss = "!f() { git show stash@{$1}; }; f"
    sp = "!f() { git stash pop stash@{$1}; }; f"
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
